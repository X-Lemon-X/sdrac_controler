{
  "$schema": "https://cdn.statically.io/gh/nguyenngoclongdev/cdn/main/schema/v11/terminal-keeper.json",
  "theme": "tribe",
  "active": "default",
  "activateOnStartup": true,
  "keepExistingTerminals": false,
  "sessions": {
    "default": [
      [
        {
          "name": "launch:develop",
          "icon": "debug",
          "color": "terminal.ansiBlue",
          "commands": [
            "source /opt/ros/humble/setup.bash",
            "colcon build --symlink-install && source install/local_setup.bash",
            "ros2 launch launch_files/develop_rc.launch.py"
          ]
        },
        {
          "name": "candump",
          "icon": "call-incoming",
          "color": "terminal.ansiGreen",
          "commands": [
            "source /opt/ros/humble/setup.bash",
            "colcon build --symlink-install && source install/local_setup.bash",
            "python3 src/sdrac_can_stranslator/sdrac_can_stranslator/can_device.py",
            "candump can0"
          ]
        }
      ],
      {
        "name": "rviz",
        "icon": "eye",
        "color": "terminal.ansiYellow",
        "commands": [
          "source /opt/ros/humble/setup.bash",
          "colcon build --symlink-install && source install/local_setup.bash",
          "export QT_SCREEN_SCALE_FACTORS=1",
          "ros2 launch sdrac_description display.launch.py"
        ]
      }
    ]
  }
}